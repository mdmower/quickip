{
  "name": "quickip",
  "version": "6.1.1",
  "description": "A browser extension to quickly find your IPv4 and IPv6 addresses",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "vite-node build/dist.ts",
    "build-debug": "vite-node build/dist.ts --debug",
    "pkg": "vite-node build/pkg.ts",
    "clean": "vite-node build/clean.ts",
    "release": "npm run lint && npm run clean && npm run build && npm run pkg",
    "test": "jest"
  },
  "repository": "github:mdmower/quickip",
  "keywords": [
    "ip",
    "ipv4",
    "ipv6",
    "quickip",
    "addon",
    "extension"
  ],
  "author": "Matthew D. Mower <mdmower@cmphys.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mdmower/quickip/issues"
  },
  "homepage": "https://github.com/mdmower/quickip#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "transform": {
      "\\.[jt]s$": [
        "ts-jest",
        {
          "useESM": true,
          "tsconfig": "tests/tsconfig.json"
        }
      ]
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "is-ip": "^5.0.1",
    "sortablejs": "^1.15.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@types/archiver": "^6.0.2",
    "@types/bootstrap": "^5.2.10",
    "@types/chrome": "0.0.268",
    "@types/fs-extra": "^11.0.4",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/jest": "^29.5.12",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.12.12",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "archiver": "^7.0.1",
    "colors": "^1.4.0",
    "css-loader": "^7.1.1",
    "deepmerge": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsdoc": "^48.2.5",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.2.0",
    "html-minifier-terser": "^7.2.0",
    "jest": "^29.7.0",
    "minimist": "^1.2.8",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "puppeteer": "^22.10.0",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.1.3",
    "ts-loader": "^9.5.1",
    "typescript": "5.4.5",
    "vite-node": "^1.6.0",
    "webpack": "^5.91.0",
    "webpack-merge": "^5.10.0"
  }
}
