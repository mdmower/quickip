{
  "name": "quickip",
  "version": "6.1.1",
  "description": "A browser extension to quickly find your IPv4 and IPv6 addresses",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "check-types": "tsc --noEmit --skipLibCheck",
    "format": "prettier --write .",
    "build": "tsx build/build.ts",
    "build-debug": "tsx build/build.ts --debug",
    "pkg": "tsx build/pkg.ts",
    "clean": "tsx build/clean.ts",
    "release": "npm run lint && npm run check-types && npm run clean && npm run build && npm run pkg",
    "test": "jest"
  },
  "repository": "github:mdmower/quickip",
  "keywords": [
    "ip",
    "ipv4",
    "ipv6",
    "quickip",
    "addon",
    "extension"
  ],
  "author": "Matthew D. Mower <mdmower@cmphys.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mdmower/quickip/issues"
  },
  "homepage": "https://github.com/mdmower/quickip#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "transform": {
      "\\.[jt]s$": [
        "ts-jest",
        {
          "useESM": true,
          "tsconfig": "tests/tsconfig.json"
        }
      ]
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "is-ip": "^5.0.1",
    "sortablejs": "^1.15.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@types/archiver": "^6.0.2",
    "@types/bootstrap": "^5.2.10",
    "@types/chrome": "0.0.268",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.14.9",
    "@types/sortablejs": "^1.15.8",
    "archiver": "^7.0.1",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "deepmerge": "^4.3.1",
    "esbuild-loader": "^4.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jsdoc": "^48.8.3",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "html-bundler-webpack-plugin": "^3.15.0",
    "jest": "^29.7.0",
    "minimist": "^1.2.8",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.2",
    "puppeteer": "^22.12.1",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "ts-jest": "^29.1.5",
    "tsx": "^4.15.7",
    "typescript": "~5.5.2",
    "typescript-eslint": "^7.17.0",
    "webpack": "^5.92.1"
  }
}
