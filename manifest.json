{
	"background": {
		"scripts": [
			"jquery-min.js",
			"ipsources.js",
			"background.js"
	]},
	"browser_action": {
		"default_icon": "icons/icon48.png",
		"default_popup": "bubble.html",
		"default_title": "Obtain your IP"
	},
	"commands": {
		"quickip-copy": {
			"suggested_key": {
				"default": "Ctrl+Shift+U",
				"mac": "Command+Shift+U"
			},
			"description": "Copy IP to clipboard"
		}
	},
	"description": "Obtain and copy your IP address",
	"icons": {
		"128": "icons/icon128.png",
		"48": "icons/icon48.png",
		"16": "icons/icon16.png"
	},
	"manifest_version": 2,
	"name": "QuickIP",
	"options_page": "options.html",
	"permissions": [
		"clipboardWrite",
		"http://checkip.dyndns.org/" ],
	"optional_permissions": [
		"http://www.checkip.org/",
		"http://ipv4.icanhazip.com/",
		"http://ifconfig.me/",
		"http://*/",
		"https://*/" ],
	"version": "2.0.1"
}